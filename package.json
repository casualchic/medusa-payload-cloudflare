{
  "name": "casual-chic-boutique-cloudflare",
  "version": "3.0.0",
  "private": true,
  "type": "module",
  "author": "Ian Rothfuss and Casual Chic Commerce LLC",
  "description": "Casual Chic Boutique Storefront with Payload CMS on Cloudflare",
  "keywords": [
    "medusa-storefront",
    "payload-cms",
    "cloudflare",
    "casual-chic-boutique",
    "casual-chic-commerce"
  ],
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "dev:local": "cross-env NODE_OPTIONS=--no-deprecation next dev -p 8000",
    "dev:cloud": "cp .env.production .env.local && cross-env NODE_OPTIONS=--no-deprecation next dev -p 8000",
    "devsafe": "rm -rf .next && rm -rf .open-next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "build": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8000\" next build",
    "build:local": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8000\" next build",
    "build:cloud": "cp .env.production .env.local && cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8000\" next build",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start -p 8000",
    "deploy": "pnpm run deploy:database && pnpm run deploy:app",
    "deploy:app": "opennextjs-cloudflare build --env=$CLOUDFLARE_ENV && opennextjs-cloudflare deploy --env=$CLOUDFLARE_ENV",
    "deploy:database": "cross-env NODE_ENV=production PAYLOAD_SECRET=ignore payload migrate && wrangler d1 execute D1 --command 'PRAGMA optimize' --env=$CLOUDFLARE_ENV --remote",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "pnpm run generate:types:cloudflare && pnpm run generate:types:payload",
    "generate:types:cloudflare": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "generate:types:payload": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "pnpm install --ignore-workspace",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview --env=$CLOUDFLARE_ENV",
    "preview:local": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "test": "pnpm run test:unit && pnpm run test:int && pnpm run test:workflows && pnpm run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=--no-deprecation pnpm exec playwright test",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts",
    "test:unit": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.unit.config.mts",
    "test:workflows": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.workflows.config.mts",
    "analyze": "ANALYZE=true next build"
  },
  "dependencies": {
    "@faceless-ui/modal": "^3.0.0",
    "@faceless-ui/scroll-info": "^2.0.0",
    "@headlessui/react": "^2.2.9",
    "@medusajs/icons": "^2.11.1",
    "@medusajs/js-sdk": "^2.11.1",
    "@medusajs/ui": "^4.0.25",
    "@opennextjs/cloudflare": "^1.11.0",
    "@payloadcms/db-d1-sqlite": "3.61.1",
    "@payloadcms/next": "3.61.1",
    "@payloadcms/richtext-lexical": "3.61.1",
    "@payloadcms/storage-r2": "3.61.1",
    "@radix-ui/react-accordion": "^1.2.12",
    "@stripe/react-stripe-js": "^5.2.0",
    "@stripe/stripe-js": "^8.1.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "graphql": "^16.11.0",
    "lodash": "^4.17.21",
    "next": "^16.0.0",
    "payload": "3.61.1",
    "react": "19.2.0",
    "react-country-flag": "^3.1.0",
    "react-dom": "19.2.0",
    "sharp": "^0.34.4",
    "tailwindcss-radix": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@medusajs/types": "^2.11.1",
    "@medusajs/ui-preset": "^2.11.1",
    "@next/eslint-plugin-next": "^16.0.0",
    "@opentelemetry/api": "^1.9.0",
    "@playwright/test": "1.56.1",
    "@tailwindcss/postcss": "^4.1.16",
    "@testing-library/react": "^16.3.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.9.1",
    "@types/react-instantsearch-dom": "^6.12.9",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-react": "^5.1.0",
    "ansi-colors": "^4.1.3",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^10.0.0",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.38.0",
    "eslint-config-next": "^16.0.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jsdom": "^27.0.1",
    "playwright": "1.56.1",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "^4.0.3",
    "webpack": "^5.102.1",
    "wrangler": "~4.45.0"
  },
  "resolutions": {
    "yjs": "^13.6.0",
    "typescript": "^5.9.3",
    "monaco-editor": "^0.53.0",
    "esbuild": "0.25.11",
    "@types/node": "^24.9.1",
    "pino": "^10.0.0"
  },
  "overrides": {
    "yjs": "^13.6.0",
    "typescript": "^5.9.3",
    "monaco-editor": "^0.53.0",
    "esbuild": "0.25.11",
    "@types/node": "^24.9.1",
    "pino": "^10.0.0"
  },
  "engines": {
    "node": ">=20.19.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "esbuild",
      "rclone.js",
      "sharp",
      "unrs-resolver",
      "workerd"
    ]
  },
  "cloudflare": {
    "bindings": {
      "PAYLOAD_SECRET": {
        "description": "Generate a random string using `openssl rand -hex 32`"
      }
    }
  }
}